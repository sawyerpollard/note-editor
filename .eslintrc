{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"globals": {
	},
	"extends": [
		"@zotero",
		"plugin:react/recommended"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 2018,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module",
		"babelOptions": {
			"configFile": "./babel.config.js"
		}
	},
	"plugins": [
		"react"
	],
	"settings": {
		"react": {
			"version": "16.14"
		}
	},
	"rules": {}
}
